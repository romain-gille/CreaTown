<div class="container">
  <div>
    <h1>Liked creations</h1>
    <% if @creations_liked.empty? %>
      <h2 class="text-center my-3">You have not liked any Creations yet</h2>
    <% else %>
      <div  class="card-container">
        <% @creations_liked.each do |creation| %>
          <%= render :partial => 'partials/card', locals: { creation: creation, user: @user } %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
