<div class="sticky_footer container">
  <h2>Add Creation</h2>
  <%= simple_form_for(creation) do |f| %>
    <%= f.input :name %>
    <%= f.input :category %>
    <%= f.input :description %>
    <%= f.input :date, as: :date, html5: true %>
    <%= f.input :photos, as: :file %>
    <%= f.submit class: "btn btn-custom" %>
  <% end %>
</div>
